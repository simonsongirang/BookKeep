CREATE DATABASE recordbook;
USE recordbook;
CREATE TABLE RECORDS (ID INT NOT NULL AUTO_INCREMENT, NAME VARCHAR(30) NOT NULL, CATEGORY VARCHAR(30) NOT NULL, CDATE DATE NOT NULL, CREDIT DECIMAL(12,2), DEBIT DECIMAL(12,2), PAYMENT_TYPE VARCHAR(30) NOT NULL, PAYEE VARCHAR(40), PAYER VARCHAR(50), NOTE VARCHAR(100), PRIMARY KEY (ID));